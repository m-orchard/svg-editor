{
  "name": "svg-editor",
  "version": "1.0.0",
  "description": "A simple SVG editor.",
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "build-js": "mkdirp dist && browserify src/main.js -o dist/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "build-css": "mkdirp dist && cp style/style.css dist"
  },
  "keywords": [
    "svg"
  ],
  "author": "Michael",
  "license": "ISC",
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.1.0",
    "@cycle/isolate": "^1.2.0",
    "rx": "^4.0.8",
    "vdom-virtualize": "https://github.com/mickylad/vdom-virtualize/tarball/style-index-fix"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "mkdirp": "^0.5.1"
  }
}
